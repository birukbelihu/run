{
  "py": {
    "name": "Python",
    "simple": ["python", "py"],
    "download": "https://www.python.org/downloads/",
    "check": "python --version",
    "run": "python {{file}}",
    "type": "interpreter"
  },

  "rb": {
    "name": "Ruby",
    "simple": ["ruby", "rb"],
    "download": "https://www.ruby-lang.org/en/downloads/",
    "check": "ruby --version",
    "run": "ruby {{file}}",
    "type": "interpreter"
  },

  "php": {
    "name": "PHP",
    "simple": ["php"],
    "download": "https://www.php.net/downloads",
    "check": "php --version",
    "run": "php {{file}}",
    "type": "interpreter"
  },

  "js": {
    "name": "JavaScript (Node.js)",
    "simple": ["javascript", "js", "node", "nodejs"],
    "download": "https://nodejs.org/en/download/",
    "check": "node --version",
    "run": "node {{file}}",
    "type": "interpreter"
  },

  "ts": {
    "name": "TypeScript",
    "simple": ["typescript", "ts"],
    "download": "https://www.typescriptlang.org/download/",
    "check": "tsc --version && node --version",
    "run": "tsc {{file}} && node {{file}}",
    "type": "compiler"
  },

  "java": {
    "name": "Java",
    "simple": ["java"],
    "download": "https://www.oracle.com/java/technologies/downloads/",
    "check": "javac -version",
    "run": "javac {{file}} && java {{class_name}}",
    "type": "compiler"
  },

  "kt": {
    "name": "Kotlin",
    "simple": ["kotlin", "kt"],
    "download": "https://kotlinlang.org/docs/command-line.html",
    "check": "kotlinc -version",
    "run": "kotlinc {{file}} -include-runtime -d {{out}} && java -jar {{out}}",
    "output": "main.jar",
    "type": "compiler"
  },

  "cs": {
    "name": "C#",
    "simple": ["csharp", "cs", "c#"],
    "download": "https://dotnet.microsoft.com/en-us/download",
    "check": "dotnet --version",
    "run": "dotnet run",
    "type": "compiler"
  },

  "rs": {
    "name": "Rust",
    "simple": ["rust", "rs"],
    "download": "https://www.rust-lang.org/tools/install",
    "check": "rustc --version",
    "run": "rustc {{file}} -o {{out}} && ./{{out}}",
    "output": "main",
    "type": "compiler"
  },

  "go": {
    "name": "Go",
    "simple": ["go", "golang"],
    "download": "https://go.dev/dl/",
    "check": "go version",
    "run": "go run {{file}}",
    "type": "compiler"
  },

  "c": {
    "name": "C",
    "simple": ["c"],
    "download": "https://gcc.gnu.org/install/",
    "check": "gcc --version",
    "run": "gcc {{file}} -o {{out}} && ./{{out}}",
    "output": "a.out",
    "type": "compiler"
  },

  "cpp": {
    "name": "C++",
    "simple": ["cpp", "c++"],
    "download": "https://gcc.gnu.org/install/",
    "check": "g++ --version",
    "run": "g++ {{file}} -o {{out}} && ./{{out}}",
    "output": "a.out",
    "type": "compiler"
  },

  "swift": {
    "name": "Swift",
    "simple": ["swift"],
    "download": "https://www.swift.org/download/",
    "check": "swift --version",
    "run": "swift {{file}}",
    "type": "compiler"
  },

  "lua": {
    "name": "Lua",
    "simple": ["lua"],
    "download": "https://www.lua.org/download.html",
    "check": "lua -v",
    "run": "lua {{file}}",
    "type": "interpreter"
  },

  "r": {
    "name": "R",
    "simple": ["r"],
    "download": "https://cran.r-project.org/",
    "check": "R --version",
    "run": "Rscript {{file}}",
    "type": "interpreter"
  },

  "jl": {
    "name": "Julia",
    "simple": ["julia", "jl"],
    "download": "https://julialang.org/downloads/",
    "check": "julia --version",
    "run": "julia {{file}}",
    "type": "interpreter"
  },

  "zig": {
    "name": "Zig",
    "simple": ["zig"],
    "download": "https://ziglang.org/download/",
    "check": "zig version",
    "run": "zig build-exe {{file}} && ./{{out}}",
    "output": "main",
    "type": "compiler"
  },

  "nim": {
    "name": "Nim",
    "simple": ["nim"],
    "download": "https://nim-lang.org/install.html",
    "check": "nim --version",
    "run": "nim c {{file}} && ./{{out}}",
    "output": "main",
    "type": "compiler"
  },

  "d": {
    "name": "D",
    "simple": ["d"],
    "download": "https://dlang.org/download.html",
    "check": "dmd --version",
    "run": "dmd {{file}} && ./{{out}}",
    "output": "main",
    "type": "compiler"
  },

  "scala": {
    "name": "Scala",
    "simple": ["scala"],
    "download": "https://www.scala-lang.org/download/",
    "check": "scala -version",
    "run": "scalac {{file}} && scala {{class_name}}",
    "type": "compiler"
  },

  "perl": {
    "name": "Perl",
    "simple": ["perl"],
    "download": "https://www.perl.org/get.html",
    "check": "perl --version",
    "run": "perl {{file}}",
    "type": "interpreter"
  },

  "haskell": {
    "name": "Haskell",
    "simple": ["haskell", "hs"],
    "download": "https://www.haskell.org/ghcup/",
    "check": "ghc --version",
    "run": "ghc {{file}} -o {{out}} && ./{{out}}",
    "output": "main",
    "type": "compiler"
  },

  "elixir": {
    "name": "Elixir",
    "simple": ["elixir", "ex"],
    "download": "https://elixir-lang.org/install.html",
    "check": "elixir --version",
    "run": "elixir {{file}}",
    "type": "interpreter"
  },

  "clojure": {
    "name": "Clojure",
    "simple": ["clojure", "clj"],
    "download": "https://clojure.org/guides/install_clojure",
    "check": "clojure --version",
    "run": "clojure {{file}}",
    "type": "interpreter"
  },

  "dart": {
    "name": "Dart",
    "simple": ["dart"],
    "download": "https://dart.dev/get-dart",
    "check": "dart --version",
    "run": "dart {{file}}",
    "type": "compiler"
  },

  "lisp": {
    "name": "Common Lisp",
    "simple": ["lisp", "common lisp", "cl"],
    "download": "https://lisp-lang.org/downloads/",
    "check": "sbcl --version",
    "run": "sbcl --script {{file}}",
    "type": "interpreter"
  },

  "bash": {
    "name": "Bash",
    "simple": ["bash", "sh", "shell"],
    "download": "https://www.gnu.org/software/bash/",
    "check": "bash --version",
    "run": "bash {{file}}",
    "type": "interpreter"
  },

  "groovy": {
    "name": "Groovy",
    "simple": ["groovy"],
    "download": "https://groovy.apache.org/download.html",
    "check": "groovy --version",
    "run": "groovy {{file}}",
    "type": "interpreter"
  },

  "ocaml": {
    "name": "OCaml",
    "simple": ["ocaml", "ml"],
    "download": "https://ocaml.org/docs/install.html",
    "check": "ocaml --version",
    "run": "ocamlc -o {{out}} {{file}} && ./{{out}}",
    "output": "a.out",
    "type": "compiler"
  }
}